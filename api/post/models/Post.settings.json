{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "default": "",
      "type": "string"
    },
    "description": {
      "default": "",
      "type": "string"
    },
    "content": {
      "default": "",
      "type": "text"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "selectedPost": {
      "default": false,
      "type": "boolean"
    },
    "user": {
      "model": "user",
      "via": "posts",
      "plugin": "users-permissions"
    },
    "category": {
      "default": "",
      "type": "enumeration",
      "required": true,
      "enum": [
        "news",
        " sports",
        " technology",
        " entertainment"
      ]
    }
  }
}